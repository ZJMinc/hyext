.container {
  height: 100%;
}
.count-down {
  position: relative;
  display: flex;
  align-items: center;
  justify-content: center;
}
.count-content {
  position: relative;
  width: 306px;
  height: 306px;
}
.img-content {
  position: absolute;
}
.img {
  width: 306;
  height: 306;
}
.dance-action {
  width: 400;
  height: 500;
}
.result-text {
  color: white;
  font-size: 150px;
  font-weight: 800;
  -webkit-text-stroke: 1px solid #ffb700;
}
