{
  "name": "hy-ext",
  "outputPath": "dist",
  "releasePath": "release",
  "extId": "094434a5fc1183e5",
  "ignore": [
    "*.md",
    "LICENSE",
    ".git/**",
    ".gitignore",
    "package-lock.json"
  ],
  "builder": {
    "name": "@hyext/builder-beyond",
    "config": {
      "webSDKVersion": "1.10.0",
      "webTitle": "虎牙小程序",
      "compileNodeModules": [
        "react-native-animatable"
      ],
      "useCssLoader": true,
      "designWidth": 750,
      "https": false,
      "host": "192.168.0.103",
      "port": 18080,
      "enableLint": false,
      "buildConfig": {
        "RN": [
          {
            "entry": "index.js",
            "extType": "app_panel",
            "platform": "app",
            "registerComponent": [
              "hy-ext"
            ]
          },
          {
            "entry": "index_streamer.js",
            "extType": "zs_anchor_panel",
            "platform": "app",
            "registerComponent": [
              "hy-ext-streamer"
            ]
          }
        ],
        "H5": [
          {
            "entry": "index_streamer.js",
            "extType": "pc_anchor_panel",
            "platform": "pc"
          },
          {
            "entry": "index.js",
            "extType": "web_video_com",
            "platform": "web"
          }
        ]
      }
    }
  }
}
